{
  "name": "contributte/apitte-skeleton",
  "description": "🎁  API / REST API / JSON API / PSR-7 / Middlewares project skeleton based on Nette Framework (@nette), Apitte (@apitte), Doctrine (@nettrine) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "api",
    "apitte",
    "sandbox",
    "project",
    "skeleton"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.1",

    "contributte/bootstrap": "^0.6.0",
    "contributte/console": "^0.9.0",
    "contributte/di": "^0.5.6",
    "contributte/http": "^0.4.0",
    "contributte/monolog": "^0.5.0",
    "contributte/neonizer": "^0.5.0",
    "contributte/security": "^0.4.0",
    "contributte/tracy": "^0.6.0",
    "contributte/utils": "^0.6.0",

    "contributte/apitte": "^0.11.0",

    "nette/finder": "~2.6.0",
    "nette/robot-loader": "~3.4.0",

    "nettrine/annotations": "^0.7.0",
    "nettrine/orm": "^0.8.0",
    "nettrine/dbal": "^0.8.0",
    "nettrine/cache": "^0.3.0",
    "nettrine/migrations": "^0.9.1",
    "nettrine/fixtures": "^0.7.2",

    "symfony/serializer": "^6.0.2",
    "symfony/validator": "^6.0.3",
    "symfony/property-info": "^6.0.0",
    "symfony/cache": "^6.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/dev": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.5.1",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "nelmio/alice": "^3.5.8"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Database\\": "db"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
