{
  "name": "contributte/apitte-skeleton",
  "description": "🎁  API / REST API / JSON API / PSR-7 / Middlewares project skeleton based on Nette Framework (@nette), Apitte (@apitte), Doctrine (@nettrine) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "api",
    "apitte",
    "sandbox",
    "project",
    "skeleton"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",

    "contributte/bootstrap": "^0.6.0",
    "contributte/console": "^0.10.1",
    "contributte/di": "^0.5.6",
    "contributte/http": "^0.4.1",
    "contributte/monolog": "^0.5.2",
    "contributte/neonizer": "^0.6.0",
    "contributte/security": "^0.4.0",
    "contributte/tracy": "^0.6.0",
    "contributte/utils": "^0.7.0",

    "contributte/apitte": "^0.12.3",

    "nettrine/annotations": "^0.7.0",
    "nettrine/orm": "^0.8.4",
    "nettrine/dbal": "^0.8.2",
    "nettrine/cache": "^0.3.0",
    "nettrine/migrations": "^0.10.0",
    "nettrine/fixtures": "^0.7.2",

    "symfony/serializer": "^7.2.3",
    "symfony/validator": "^7.2.3",
    "symfony/property-info": "^7.2.3",
    "symfony/cache": "^7.2.3"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/dev": "^0.5",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.3",
    "mockery/mockery": "^1.6.12",
    "phpstan/phpstan-doctrine": "^2.0.0",
    "nelmio/alice": "^3.14.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Database\\": "db"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
